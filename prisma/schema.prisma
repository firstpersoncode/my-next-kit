generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./sqlite.db"
}

model session {
  id             String   @id @default(cuid())
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  exp            DateTime
  sjwt           String @unique
  source         String
  deviceType     String?
  deviceVendor   String?
  os             String?
  osVersion      Int?
  browser        String?
  browserVersion Int?
  isBot          Boolean?

  @@map("Session")
}